   "source": [
    "class Solution:\n",
    "    def closeStrings(self, word1: str, word2: str) -> bool:\n",
    "        c1=Counter(word1)\n",
    "        c2=Counter(word2)\n",
    "        n1=Counter(c1.values())\n",
    "        n2=Counter(c2.values())\n",
    "        return c1==c2 or (n1==n2 and set(word1)==set(word2))"
   ]
