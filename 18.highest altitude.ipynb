   "source": [
    "class Solution:\n",
    "    def largestAltitude(self, gain: List[int]) -> int:\n",
    "        ca=0\n",
    "        hp=ca\n",
    "\n",
    "        for i in gain:\n",
    "            ca = ca+i\n",
    "            hp = max(hp,ca)\n",
    "        return hp"
   ]
  }
