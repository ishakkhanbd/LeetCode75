   "source": [
    "class Solution:\n",
    "    def longestOnes(self, nums: List[int], k: int) -> int:\n",
    "        i = 0\n",
    "        for j in range(len(nums)):\n",
    "            k -= 1 - nums[j]\n",
    "            if k < 0:\n",
    "                k += 1 - nums[i]\n",
    "                i += 1\n",
    "        return j - i + 1"
   ]
