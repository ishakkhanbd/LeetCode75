   "source": [
    "class Solution:\n",
    "    def maxArea(self, h: List[int]) -> int:\n",
    "        n = len(h)\n",
    "        l, r = 0, n - 1\n",
    "        ans = 0\n",
    "        while l <= r:\n",
    "            ans = max(ans, (r - l) * min(h[l], h[r]))\n",
    "            if h[l] < h[r]: l += 1\n",
    "            else:           r -= 1\n",
    "        return ans"
   ]
  }
