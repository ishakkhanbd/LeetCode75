   "source": [
    "class Solution:\n",
    "    def removeStars(self, s: str) -> str:\n",
    "        ans=[]\n",
    "        for i in s:\n",
    "            if i=='*':\n",
    "                ans.pop()\n",
    "            else:\n",
    "                ans+=[i]\n",
    "        return \"\".join(ans)"
   ]
  }
